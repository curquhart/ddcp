{
  "name": "@ddcp/github-mirror",
  "private": true,
  "version": "1.0.0",
  "main": "dist/bundled.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf node_modules dist .cache",
    "lint": "eslint --ext .ts src/",
    "pack": "parcel build --target node --bundle-node-modules --no-source-maps --out-file dist/bundled.js dist/index.js && parcel build --target node --bundle-node-modules --no-source-maps --out-file dist/git-credentials-helper-bundled.js dist/git-credentials-helper.js && npm-pack-zip --dst dist/"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "eslintConfig": {
    "extends": [
      "@ddcp/eslint-config"
    ]
  },
  "dependencies": {
    "@octokit/webhooks": "7.1.0",
    "@types/aws4": "1.5.1",
    "@types/rimraf": "2.0.3",
    "@types/shell-escape": "0.2.0",
    "aws4": "1.9.1",
    "github-webhook-validators": "1.0.3",
    "npm-pack-zip": "1.2.7",
    "rimraf": "3.0.2",
    "shell-escape": "0.2.0",
    "tmp": "0.1.0",
    "aws-lambda": "1.0.5"
  },
  "devDependencies": {
    "@types/node": "13.7.4",
    "@types/tmp": "0.1.0",
    "@types/aws-lambda": "8.10.46",
    "eslint": "6.8.0",
    "typescript": "3.8.2",
    "@typescript-eslint/eslint-plugin": "2.21.0",
    "@typescript-eslint/parser": "2.21.0",
    "eslint-plugin-node": "11.0.0",
    "parcel": "1.12.4",
    "@ddcp/eslint-config": "1.0.0",
    "@ddcp/typescript-config": "1.0.0"
  }
}