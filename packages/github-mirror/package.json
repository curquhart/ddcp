{
  "name": "@ddcp/github-mirror",
  "private": true,
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "bundledDependencies": [
    "aws4",
    "rimraf",
    "shell-escape",
    "github-webhook-validators",
    "tmp"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist node_modules",
