{
  "name": "@ddcp/github-mirror",
  "version": "1.14.0-240",
  "main": "dist/bundled.js",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf node_modules dist .cache",
    "lint": "eslint --ext .ts src/",
    "prepack": "parcel build --target node --bundle-node-modules --no-source-maps --out-file dist/bundled.js dist/index.js && parcel build --target node --bundle-node-modules --no-source-maps --out-file dist/git-credentials-helper-bundled.js dist/git-credentials-helper.js",
    "pack": "npm-pack-zip --dst dist/",
    "test": "jest"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "eslintConfig": {
    "extends": [
      "@ddcp/eslint-config"
    ]
  },
  "dependencies": {
    "@ddcp/logger": "^1.14.0-240",
    "@ddcp/secretsmanager": "^1.14.0-240",
    "@octokit/webhooks": "7.1.0",
    "aws-lambda": "1.0.5",
    "aws4": "1.9.1",
    "github-webhook-validators": "1.0.3",
    "npm-pack-zip": "1.2.7",
    "rimraf": "3.0.2",
    "shell-escape": "0.2.0",
    "tmp": "0.1.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.9.0",
    "@ddcp/eslint-config": "^1.14.0-240",
    "@ddcp/typescript-config": "^1.14.0-240",
    "@types/aws-lambda": "8.10.46",
    "@types/aws4": "1.5.1",
    "@types/jest": "25.1.4",
    "@types/node": "13.7.4",
    "@types/rimraf": "2.0.3",
    "@types/shell-escape": "0.2.0",
    "@types/tmp": "0.1.0",
    "@typescript-eslint/eslint-plugin": "2.21.0",
    "@typescript-eslint/parser": "2.21.0",
    "eslint": "^7.0.0-alpha.3",
    "eslint-plugin-node": "11.0.0",
    "jest": "25.1.0",
    "parcel": "1.12.4",
    "ts-jest": "25.2.1",
    "typescript": "3.8.2"
  }
}
