{
  "name": "@ddcp/manager",
  "version": "1.4.0-121",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "mkdir -p dist && cdk synth > dist/manager.yaml",
    "clean": "rm -rf node_modules dist",
    "lint": "eslint --ext .ts src/",
    "artifact-lint": "cfn-lint -i W3005 -- dist/manager.yaml && cfn_nag_scan --fail-on-warnings --input-path dist/manager.yaml",
    "test": "jest"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "eslintConfig": {
    "extends": [
      "@ddcp/eslint-config"
    ]
  },
  "dependencies": {
    "@aws-cdk/aws-cloudformation": "1.25.0",
    "@aws-cdk/aws-codebuild": "1.25.0",
    "@aws-cdk/aws-codecommit": "1.25.0",
    "@aws-cdk/aws-codepipeline": "1.25.0",
    "@aws-cdk/aws-codepipeline-actions": "1.25.0",
    "@aws-cdk/aws-events": "1.25.0",
    "@aws-cdk/aws-events-targets": "1.25.0",
    "@aws-cdk/aws-iam": "1.25.0",
    "@aws-cdk/aws-lambda": "1.25.0",
    "@aws-cdk/aws-s3": "1.25.0",
    "@aws-cdk/aws-sns": "1.25.0",
    "@aws-cdk/core": "1.25.0",
    "@aws-cdk/cx-api": "1.25.0",
    "@ddcp/module-collection": "^1.4.0-121",
    "aws-cdk": "1.25.0",
    "js-yaml": "3.13.1",
    "tmp": "0.1.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.9.0",
    "@ddcp/eslint-config": "^1.4.0-121",
    "@ddcp/s3-resolver": "^1.4.0-121",
    "@ddcp/typescript-config": "^1.0.0",
    "@types/jest": "25.1.4",
    "@types/js-yaml": "3.12.1",
    "@types/node": "13.7.4",
    "@types/tmp": "0.1.0",
    "@typescript-eslint/eslint-plugin": "2.21.0",
    "@typescript-eslint/parser": "2.21.0",
    "eslint": "6.8.0",
    "eslint-plugin-node": "11.0.0",
    "jest": "25.1.0",
    "ts-jest": "25.2.1",
    "ts-node": "8.6.2",
    "typescript": "3.8.2"
  }
}
