{
  "name": "@ddcp/action-counter",
  "version": "1.11.0-204",
  "main": "dist/bundled.js",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf node_modules dist .cache",
    "lint": "eslint --ext .ts src/",
    "prepack": "parcel build --target node --bundle-node-modules --no-source-maps --out-file dist/bundled.js dist/index.js",
    "pack": "npm-pack-zip --dst dist/",
    "test": "jest"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "targets": {
    "main": {
      "context": "node",
      "engines": {
        "node": "12.0.0"
      },
      "includeNodeModules": true
    }
  },
  "eslintConfig": {
    "extends": [
      "@ddcp/eslint-config"
    ]
  },
  "dependencies": {
    "@ddcp/logger": "^1.11.0-204",
    "adm-zip": "0.4.14",
    "aws-lambda": "1.0.5",
    "aws-sdk": "2.641.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.9.0",
    "@ddcp/eslint-config": "^1.11.0-204",
    "@ddcp/typescript-config": "^1.11.0-204",
    "@types/adm-zip": "0.4.32",
    "@types/aws-lambda": "8.10.44",
    "@types/jest": "25.1.4",
    "@typescript-eslint/eslint-plugin": "2.21.0",
    "@typescript-eslint/parser": "2.21.0",
    "aws-sdk-mock": "5.1.0",
    "eslint": "^7.0.0-alpha.3",
    "eslint-plugin-node": "11.0.0",
    "jest": "25.1.0",
    "npm-pack-zip": "1.2.7",
    "parcel": "1.12.4",
    "ts-jest": "25.2.1",
    "typescript": "3.8.2"
  }
}
