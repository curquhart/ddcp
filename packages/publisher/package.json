{
  "name": "@ddcp/publisher",
  "version": "0.0.1-alpha.48",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf node_modules dist",
    "deploy": "cdk deploy",
    "lint": "eslint --ext .ts src/",
    "pack": "zip -jr dist/dist.zip node_modules/@ddcp/action-counter/dist/@ddcpaction-counter.zip node_modules/@ddcp/github-mirror/dist/@ddcpgithub-mirror.zip node_modules/@ddcp/synthesizer/dist/@ddcpsynthesizer.zip node_modules/@ddcp/sns-to-slack/dist/@ddcpsns-to-slack.zip node_modules/@ddcp/sns-to-github/dist/@ddcpsns-to-github.zip node_modules/@ddcp/github-mirror/dist/@ddcpgithub-mirror.zip node_modules/@ddcp/manager/dist/manager.yaml",
    "artifact-lint": "check-zip dist/dist.zip @ddcpsynthesizer.zip @ddcpaction-counter.zip @ddcpgithub-mirror.zip @ddcpsns-to-slack.zip @ddcpsns-to-github.zip @ddcpgithub-mirror.zip manager.yaml"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "eslintConfig": {
    "extends": [
      "@ddcp/eslint-config"
    ]
  },
  "dependencies": {
    "@aws-cdk/aws-cloudformation": "1.25.0",
    "@aws-cdk/aws-codecommit": "1.25.0",
    "@aws-cdk/aws-events": "1.25.0",
    "@aws-cdk/aws-iam": "1.25.0",
    "@aws-cdk/aws-lambda": "1.25.0",
    "@aws-cdk/aws-s3": "1.25.0",
    "@aws-cdk/aws-s3-deployment": "1.25.0",
    "@aws-cdk/core": "1.25.0",
    "@aws-cdk/cx-api": "1.25.0",
    "aws-cdk": "1.25.0",
    "npm-pack-zip": "1.2.7"
  },
  "devDependencies": {
    "@ddcp/action-counter": "^0.0.1-alpha.48",
    "@ddcp/eslint-config": "^0.0.1-alpha.48",
    "@ddcp/github-mirror": "^0.0.1-alpha.48",
    "@ddcp/manager": "^0.0.1-alpha.48",
    "@ddcp/sns-to-github": "^0.0.1-alpha.48",
    "@ddcp/sns-to-slack": "^0.0.1-alpha.48",
    "@ddcp/synthesizer": "^0.0.1-alpha.48",
    "@ddcp/tools": "^0.0.1-alpha.48",
    "@ddcp/typescript-config": "^0.0.1-alpha.48",
    "@typescript-eslint/eslint-plugin": "2.21.0",
    "@typescript-eslint/parser": "2.21.0",
    "eslint": "6.8.0",
    "eslint-plugin-node": "11.0.0",
    "ts-node": "8.6.2",
    "typescript": "3.8.2"
  }
}
