{
  "name": "@ddcp/publisher",
  "private": true,
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf node_modules dist",
    "deploy": "npm run pack && cdk -o dist/cdk.out deploy --path-metadata false --version-reporting false",
    "lint": "eslint --ext .ts src/",
    "pack": "rm -rf dist && npm run build && zip -jr dist/dist.zip node_modules/@ddcp/synthesizer/dist/@ddcpsynthesizer.zip node_modules/@ddcp/github-mirror/dist/@ddcpgithub-mirror.zip node_modules/@ddcp/manager/dist/manager.yaml"
  },
  "engines": {
    "node": ">=11.13.0"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc"
    ]
  },
  "dependencies": {
    "@aws-cdk/cx-api": "1.25.0",
    "@aws-cdk/aws-cloudformation": "1.25.0",
    "@aws-cdk/aws-codecommit": "1.25.0",
    "@aws-cdk/aws-events": "1.25.0",
    "@aws-cdk/aws-iam": "1.25.0",
    "@aws-cdk/aws-lambda": "1.25.0",
    "@aws-cdk/aws-s3": "1.25.0",
    "@aws-cdk/aws-s3-deployment": "1.25.0",
    "@aws-cdk/core": "1.25.0",
    "aws-cdk": "1.25.0",
    "npm-pack-zip": "1.2.7",
    "@aws-cdk/aws-apigateway": "1.25.0"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "ts-node": "8.6.2",
    "typescript": "3.8.2",
    "@ddcp/manager": "1.0.0",
    "@ddcp/synthesizer": "1.0.0",
    "@ddcp/github-mirror": "1.0.0"
  }
}
